<?xml-stylesheet type='text/xsl' href='./CodelistDictionary-v32.xsl'?><gml:Dictionary xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="VehicleObstructionTypeList" xsi:schemaLocation="http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">
  <gml:description>Types of obstructions involving vehicles.</gml:description>
  <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists">VehicleObstructionTypeList</gml:identifier>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="abandonedVehicle">
      <gml:description>Abandoned vehicle(s) on the roadway which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">abandonedVehicle</gml:identifier>
      <gml:name>0</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="abnormalLoad">
      <gml:description>Vehicle(s) carrying abnormal indivisible load(s) which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">abnormalLoad</gml:identifier>
      <gml:name>1</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="brokenDownVehicle">
      <gml:description>Broken down vehicle(s) on the carriageway which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">brokenDownVehicle</gml:identifier>
      <gml:name>2</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="caterpillarTrackedVehicle">
      <gml:description>Caterpillar tracked vehicle(s) are in use which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">caterpillarTrackedVehicle</gml:identifier>
      <gml:name>17</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="convoy">
      <gml:description>A group of vehicles moving together in formation which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">convoy</gml:identifier>
      <gml:name>3</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="damagedVehicle">
      <gml:description>Damaged vehicle(s) on the carriageway which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">damagedVehicle</gml:identifier>
      <gml:name>4</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="dangerousSlowMovingVehicle">
      <gml:description>Dangerous slow moving vehicle(s) which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">dangerousSlowMovingVehicle</gml:identifier>
      <gml:name>5</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="emergencyVehicle">
      <gml:description>Emergency service vehicle(s) on the roadway in response to an emergency situation.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">emergencyVehicle</gml:identifier>
      <gml:name>6</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="highSpeedChase">
      <gml:description>Authorised and unauthorised vehicles are travelling at high speeds along the roadway. This can present a hazard to other vehicles.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">highSpeedChase</gml:identifier>
      <gml:name>9</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="highSpeedEmergencyVehicle">
      <gml:description>Emergency service vehicle(s) progressing at high speed along the roadway in response to or en route from an emergency situation.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">highSpeedEmergencyVehicle</gml:identifier>
      <gml:name>7</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="longLoad">
      <gml:description>A vehicle of length greater than that normally allowed which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">longLoad</gml:identifier>
      <gml:name>8</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="militaryConvoy">
      <gml:description>A group of military vehicles moving together in formation which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">militaryConvoy</gml:identifier>
      <gml:name>11</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="other">
      <gml:description>Other than as defined in this code list.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">other</gml:identifier>
      <gml:name>29</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="prohibitedVehicleOnTheRoad">
      <gml:description>Vehicle(s) not normally permitted on the motorway are present which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">prohibitedVehicleOnTheRoad</gml:identifier>
      <gml:name>13</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="recklessDriver">
      <gml:description>A vehicle being driven without due care and attention is causing a hazard to other vehicles.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">recklessDriver</gml:identifier>
      <gml:name>14</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="slowVehicle">
      <gml:description>A vehicle travelling at well below normal highway speeds which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">slowVehicle</gml:identifier>
      <gml:name>15</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="specialPermitTransport">
      <gml:description>Special type of load or vehicle size, or even speed, that is allowed on the road only with special permission. The presence of this transport can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">specialPermitTransport</gml:identifier>
      <gml:name>16</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="unlitVehicleOnTheRoad">
      <gml:description>Vehicle(s) without lights are in use which can present a hazard to road users.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">unlitVehicleOnTheRoad</gml:identifier>
      <gml:name>19</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="vehicleCarryingDangerousGoods">
      <gml:description>Vehicle(s) carrying materials of a hazardous nature are present and these could expose road users to additional hazards.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">vehicleCarryingDangerousGoods</gml:identifier>
      <gml:name>21</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="vehicleInDifficulty">
      <gml:description>A vehicle is experiencing difficulties (e.g. manoeuvring or propulsion difficulties) which can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">vehicleInDifficulty</gml:identifier>
      <gml:name>22</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="vehicleOnFire">
      <gml:description>A vehicle is or has been on fire and can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">vehicleOnFire</gml:identifier>
      <gml:name>20</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="vehicleOnWrongCarriageway">
      <gml:description>A vehicle is travelling the wrong way along a divided highway (i.e. on the wrong side).</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">vehicleOnWrongCarriageway</gml:identifier>
      <gml:name>23</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="vehicleStuck">
      <gml:description>One or more vehicles are stuck (i.e. unable to move) due to environmental conditions such as a snow drift or severe icy road.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">vehicleStuck</gml:identifier>
      <gml:name>24</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="winterMaintenanceVehicleInTransfer">
      <gml:description>Winter maintenance vehicle is on the road, not doing its specific work. Its presence can cause traffic disruption.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/VehicleObstructionType">winterMaintenanceVehicleInTransfer</gml:identifier>
      <gml:name>28</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
</gml:Dictionary>
