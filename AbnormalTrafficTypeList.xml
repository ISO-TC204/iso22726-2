<?xml-stylesheet type='text/xsl' href='./CodelistDictionary-v32.xsl'?><gml:Dictionary xmlns:gml="http://www.opengis.net/gml/3.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" gml:id="AbnormalTrafficTypeList" xsi:schemaLocation="http://www.opengis.net/gml/3.2 http://schemas.opengis.net/gml/3.2.1/gml.xsd">
  <gml:description>Descriptive terms for abnormal traffic conditions specifically relating to the nature of the traffic movement, implying levels of service.</gml:description>
  <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists">AbnormalTrafficTypeList</gml:identifier>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="heavyTraffic">
      <gml:description>Traffic is heavy at the specified location (i.e. average speed is between 75% and 90% of its free-flow level).</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/AbnormalTrafficTypeList">heavyTraffic</gml:identifier>
      <gml:name>3</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="queuingTraffic">
      <gml:description>Traffic is queuing at the specified location, although there is still some traffic movement (i.e. average speed is between 10% and 25% of its free-flow level).</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/AbnormalTrafficTypeList">queuingTraffic</gml:identifier>
      <gml:name>1</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="slowTraffic">
      <gml:description>Traffic is slow moving at the specified location, but not yet forming queues (i.e. average speed is between 25% and 75% of its free-flow level).</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/AbnormalTrafficTypeList">slowTraffic</gml:identifier>
      <gml:name>2</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="stationaryTraffic">
      <gml:description>Traffic is stationary, or very near stationary, at the specified location (i.e. average speed is less than 10% of its free-flow level).</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/AbnormalTrafficTypeList">stationaryTraffic</gml:identifier>
      <gml:name>0</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
  <gml:dictionaryEntry>
    <gml:Definition gml:id="unspecified">
      <gml:description>There are abnormal traffic conditions of an unspecified nature at the specified location.</gml:description>
      <gml:identifier codeSpace="https://standards.iso.org/iso/22726/-2/ed-1/en/codelists/AbnormalTrafficTypeList">unspecified</gml:identifier>
      <gml:name>4</gml:name>
    </gml:Definition>
  </gml:dictionaryEntry>
</gml:Dictionary>
